                        <efext:EformsExtension>
                            <efbc:FrameworkMaximumAmount currencyID="EUR">750000</efbc:FrameworkMaximumAmount>
                        </efext:EformsExtension>
                    </ext:ExtensionContent>
                </ext:UBLExtension>
            </ext:UBLExtensions>
            <cbc:EstimatedOverallContractAmount currencyID="EUR">750000</cbc:EstimatedOverallContractAmount>
        </cac:RequestedTenderTotal>
